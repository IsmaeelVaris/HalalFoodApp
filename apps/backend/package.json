{
  "name": "backend",
  "private": true,
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@prisma/client": "^5.22.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prisma": "^5.22.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "prisma:watch": "onchange 'prisma/schema.prisma' -- npx prisma generate",
    "predev": "prisma generate && prisma migrate dev",
    "dev": "nodemon --watch src --ext ts --exec \"pnpm exec ts-node src/index.ts\"",
    "build": "tsc -b",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "start": "node dist/index.js",
    "studio": "prisma studio"
  },
  "version": "1.0.0",
  "description": "nodemon is a development tool for Node.js that automatically restarts your server whenever your source files change.",
  "main": "server.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
