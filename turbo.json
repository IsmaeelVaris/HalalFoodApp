{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["pnpm-lock.yaml"],
  "tasks": {
    "dev": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "outputs": []
    }
  }
}
